    "nodemon": "^3.1.4",
    "qrcode": "^1.5.3",
    "redis": "^4.7.0",
    "socks-proxy-agent": "^8.0.4",
    "tonweb": "^0.0.66",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.14.12",
    "@types/uuid": "^10.0.0",
    "prettier": "3.3.3",
